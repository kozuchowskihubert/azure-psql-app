{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": ["app/**"]
      }
    },
    {
      "src": "app/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|mp3|wav|ogg|json)",
      "dest": "/app/public/$1.$2"
    },
    {
      "src": "/(.*)\\.html",
      "dest": "/app/public/$1.html"
    },
    {
      "src": "/",
      "dest": "/app/public/index.html"
    },
    {
      "src": "/haos-platform(.*)",
      "dest": "/app/public/haos-platform$1"
    },
    {
      "src": "/presets",
      "dest": "/app/public/presets.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
